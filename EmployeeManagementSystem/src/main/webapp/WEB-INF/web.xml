<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>EmployeeManagementSystem</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>AddStaffController</servlet-name>
    <servlet-class>com.parkmanagement.controller.AddStaffController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddStaffController</servlet-name>
    <url-pattern>/addstaff</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>createroutinecontroller</servlet-name>
    <servlet-class>com.parkmanagement.controller.createroutinecontroller</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>createroutinecontroller</servlet-name>
    <url-pattern>/createRoutine</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logincontroller</servlet-name>
    <servlet-class>com.parkmanagement.controller.logincontroller</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logincontroller</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewStaff</servlet-name>
    <servlet-class>com.parkmanagement.controller.ViewStaff</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewStaff</servlet-name>
    <url-pattern>/viewstaff</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewRoutine</servlet-name>
    <servlet-class>com.parkmanagement.controller.ViewRoutine</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewRoutine</servlet-name>
    <url-pattern>/ViewRout</url-pattern>
  </servlet-mapping>
</web-app>